{
  "name": "@byte-power/json-press",
  "title": "JSONPress",
  "description": "JSON Schema based editor",
  "version": "1.1.3-beta.16",
  "main": "dist/jsonpress.js",
  "author": {
    "name": "lizux"
  },
  "bugs": {
    "url": "https://github.com/byte-power/jsonpress/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/byte-power/jsonpress.git"
  },
  "keywords": [
    "json",
    "schema",
    "jsonschema",
    "editor"
  ],
  "scripts": {
    "pub": "npm version minor",
    "push": "npm version prerelease --preid beta --no-git-tag-version",
    "prepare": "npm run build",
    "clean": "find ./src -name *.css.js -type f -delete",
    "build": "webpack --config config/webpack.prod.js --progress --profile --bail",
    "build:dev": "webpack --config config/webpack.dev.js --progress --profile --bail",
    "watch": "webpack --config config/webpack.dev.js --watch",
    "watch:css": "nodemon --watch ./src/ --ext css,less --ignore dist --exec 'npm run build:dev'",
    "debug": "webpack-dev-server --config config/webpack.dev.js --progress",
    "test": "karma start config/karma.conf.js --browsers Chrome --log-level debug",
    "test-headless": "karma start config/karma.conf.js --singleRun true --browsers ChromeHeadless",
    "serve-test": "http-server --p 9001",
    "docker-test": "docker-compose run --rm node npm install && docker-compose run --rm node npm run build && docker-compose up -d && docker-compose ps && docker-compose exec node codeceptjs -c /repo/tests/codeceptjs/codecept.json run-multiple basic:chrome --invert --grep '@optional'",
    "preversion": "npm run test-headless",
    "postversion": "git push && git push --tags && npm publish ./ --tag beta --access public",
    "winserver": "cd tests && start cmd /c npm run serve-test .",
    "cp:test": "codeceptjs run --steps --invert --grep  @optional",
    "cp:testmocha": "codeceptjs run --steps --reporter mochawesome --invert --grep  @optional",
    "cp:fulltest": "codeceptjs run --steps",
    "cp:fulltestmocha": "codeceptjs run --steps --reporter mochawesome",
    "eslint.fix": "eslint -c ./.eslintrc src/**/*.js --fix",
    "eslint": "eslint -c ./.eslintrc src/**/*.js"
  },
  "license": "MIT",
  "engines": {
    "node": ">= 0.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.9.6",
    "@babel/runtime": "^7.12.1",
    "ace-builds": "^1.4.11",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "cleave.js": "^1.5.10",
    "codeceptjs": "^2.6.2",
    "css-loader": "^3.5.3",
    "css2json": "^1.1.1",
    "cssnano": "^4.1.10",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "http-server": "^0.12.3",
    "jasmine": "^3.5.0",
    "jasmine-core": "^3.5.0",
    "jodit": "^3.3.24",
    "jquery": "^3.5.0",
    "karma": "^5.0.4",
    "karma-chrome-launcher": "^3.1.0",
    "karma-jasmine": "^2.0.1",
    "karma-jasmine-html-reporter": "^1.5.3",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^4.0.2",
    "less": "^4.1.1",
    "mathjs": "^6.6.4",
    "mini-css-extract-plugin": "^0.8.2",
    "mocha": "^6.2.2",
    "mochawesome": "^4.1.0",
    "nodemon": "^2.0.7",
    "postcss-loader": "^3.0.0",
    "puppeteer": "^1.20.0",
    "remove-strict-webpack-plugin": "^0.1.2",
    "sceditor": "^2.1.3",
    "sinon": "^8.1.1",
    "standard": "^14.3.3",
    "style-loader": "^1.2.1",
    "terser-webpack-plugin": "^4.2.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "core-js": "^3.6.5"
  }
}
